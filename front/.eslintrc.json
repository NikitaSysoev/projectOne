{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true
  },
  "extends": ["airbnb"],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true,
      "experimentalObjectRestSpread": true
    }
  },
  "plugins": ["react"],
  "rules": {
    "react/jsx-filename-extension": "off",
    "import/no-named-as-default": 0,
    "import/no-unresolved": "off",
    "arrow-parens": 0,
    "comma-dangle": 0,
    "no-console": 0,
    "no-unused-expressions": 0,
    "object-curly-newline": 0,
    "react/prefer-stateless-function": 0,
    "arrow-body-style": 0,
    "no-underscore-dangle": 0,
    "no-case-declarations": 0,
    "no-param-reassign": 0,
    "react/prop-types": 0
  },
  "globals": {
    "it": false
  }
}
