{
    "env": {
      "browser": true,
      "node": true,
      "commonjs": true,
      "es6": true
    },
    "extends": ["airbnb"],
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "rules": {
      "import/no-named-as-default": 0
    },
    "settings": {
      "ecmascript": 6,
      "import/parser": "babel-eslint"
    }
  }