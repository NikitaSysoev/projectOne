{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true
  },
  "extends": ["airbnb"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "react/jsx-filename-extension": "off",
    "import/no-named-as-default": 0
  },
  "globals": {
    "it": false
  },
  "settings": {
    "ecmascript": 6,
    "import/parser": "babel-eslint"
  }
}
