{
    "env": {
      "browser": true,
      "node": true,
      "commonjs": true,
      "es6": true,
      "jest": true
    },
    "extends": ["airbnb"],
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "rules": {
      "import/no-named-as-default": 0,
      "no-console": 0,
      "arrow-parens": 0,
      "comma-dangle": 0
    }
  }